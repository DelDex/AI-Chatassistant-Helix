{
    "name": "sample/chatbot-plugin",
    "description": "Sample PHP chatbot plugin forwarding messages to n8n webhook",
    "type": "project",
    "require": {
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
